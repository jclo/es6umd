{
  "name": "es6umd",
  "version": "0.5.0beta1",
  "description": "A template for writing micro ES6 Javascript libraries",
  "bin": {
    "es6umd": "./bin/es6umd.js"
  },
  "main": "_dist/es6umd.js",
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp",
    "test": "NODE_ENV=test nyc --reporter=lcov _mocha ./test/main.js --compilers js:babel-core/register",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "report": "nyc report",
    "precommit": "npm run build && npm run test && npm run check-coverage && npm run report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/es6umd.git"
  },
  "keywords": [
    "ES6",
    "UMD",
    "library",
    "template",
    "boilerplate"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/es6umd/issues"
  },
  "homepage": "https://github.com/jclo/es6umd",
  "dependencies": {
    "nopt": "4.0.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "4.1.4",
    "babel-preset-latest": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "chai": "4.0.2",
    "coveralls": "2.13.1",
    "del": "3.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-header": "1.8.8",
    "gulp-replace": "0.6.1",
    "gulp-sourcemaps": "2.6.0",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "husky": "0.13.4",
    "mocha": "3.4.2",
    "nyc": "10.3.0",
    "opener": "1.4.3",
    "require-dir": "0.3.2",
    "run-sequence": "1.2.2",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0"
  },
  "browser": {},
  "browserify-shim": {},
  "browserify-swap": {},
  "browserify": {},
  "engines": {
    "node": ">=4.0.0"
  }
}

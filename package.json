{
  "name": "es6umd",
  "version": "0.6.2",
  "description": "A template for writing micro ES6 Javascript libraries",
  "bin": {
    "es6umd": "./bin/es6umd.js"
  },
  "main": "_dist/es6umd.js",
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp",
    "makeprivate": "gulp makeprivatepackage && sh tasks/compress.sh $npm_package_name $npm_package_version",
    "test": "NODE_ENV=test nyc --reporter=lcov _mocha ./test/main.js --compilers js:babel-core/register",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "report": "nyc report",
    "precommit": "npm run build && npm run test && npm run check-coverage && npm run report",
    "doc": "cd docs && ../node_modules/.bin/gitbook --port 9999 serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/es6umd.git"
  },
  "keywords": [
    "ES6",
    "UMD",
    "library",
    "template",
    "boilerplate"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/es6umd/issues"
  },
  "homepage": "https://github.com/jclo/es6umd",
  "dependencies": {
    "nopt": "4.0.1",
    "path": "0.12.7"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-add-module-exports": "1.0.0",
    "babel-plugin-istanbul": "5.0.1",
    "babel-preset-env": "1.7.0",
    "babelify": "8.0.0",
    "browserify": "16.2.2",
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "del": "3.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "gitbook-cli": "2.3.2",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.1",
    "gulp-header": "2.0.5",
    "gulp-replace": "1.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.1",
    "gulp-util": "3.0.8",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "opener": "1.5.1",
    "require-dir": "1.0.0",
    "run-sequence": "2.2.1",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0",
    "watchify": "3.11.0"
  },
  "browser": {},
  "browserify-shim": {},
  "browserify-swap": {},
  "browserify": {},
  "engines": {
    "node": ">=4.0.0"
  },
  "private": false
}

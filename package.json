{
  "name": "es6umd",
  "version": "0.3.0",
  "description": "A template for writing micro ES6 Javascript libraries",
  "bin": {
    "es6umd": "./bin/es6umd.js"
  },
  "main": "_dist/es6umd.js",
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "test": "istanbul cover -x ./test/main.js _mocha -- -R spec ./test/main.js --compilers js:babel/register",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jclo/es6umd.git"
  },
  "keywords": [
    "ES6",
    "UMD",
    "library",
    "template",
    "boilerplate"
  ],
  "author": {
    "name": "jclo",
    "email": "jclo@mobilabs.fr",
    "url": "http://www.mobilabs.fr/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/es6umd/issues"
  },
  "homepage": "https://github.com/jclo/es6umd",
  "dependencies": {
    "nopt": "3.0.6",
    "path": "0.12.7"
  },
  "devDependencies": {
    "babel": "5.8.38",
    "babelify": "6.3.0",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "del": "2.2.2",
    "eslint": "3.5.0",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.15.0",
    "ghooks": "1.3.2",
    "gulp": "3.9.1",
    "gulp-header": "1.8.8",
    "gulp-replace": "0.5.4",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.7",
    "istanbul": "0.4.5",
    "mocha": "3.1.0",
    "opener": "1.4.2",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0"
  },
  "browser": {},
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
